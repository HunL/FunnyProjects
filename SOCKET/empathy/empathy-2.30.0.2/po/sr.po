msgid "Invisible"
msgstr "Невидљив"

#: ../libempathy/empathy-utils.c:247
msgid "Offline"
msgstr "Неповезан"

#: ../libempathy/empathy-utils.c:286
msgid "No reason specified"
msgstr "Није наведен разлог"

