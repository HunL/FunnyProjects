msgstr "Tiêu chuẩn sắp xếp danh sách liên hệ"

#: ../data/empathy.schemas.in.h:8
msgid "Default directory to select an avatar image from"
msgstr "Thư mục mặc định từ đó cần chọn một ảnh riêng"

